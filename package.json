{
	"name": "pages-cms-template",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"predev": "pnpm generate:types && pnpm generate:data",
		"build": "vite build",
		"prebuild": "pnpm generate:types && pnpm generate:data",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"vite-run": "node ./vite-run.js",
		"generate:types": "pnpm vite-run ./src/lib/db/codegen.ts",
		"generate:data": "pnpm vite-run ./src/lib/export-data/index.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.45.3",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/eslint": "^9.6.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.9",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"vite-node": "^2.1.5",
		"vitest": "^2.0.4"
	},
	"dependencies": {
		"@inlang/paraglide-sveltekit": "^0.11.1",
		"@sveltejs/adapter-static": "^3.0.6",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"dotenv": "^16.4.5",
		"effect": "^3.10.15",
		"flat": "^6.0.1",
		"html-entities": "^2.5.2",
		"marked": "^15.0.1",
		"runed": "^0.15.3",
		"type-fest": "^4.27.0",
		"yaml": "^2.6.1"
	}
}
